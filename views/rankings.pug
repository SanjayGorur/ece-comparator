extends layout

block content
  .row
    .col-sm-6
      h2 Current Classes Ranked in Order âš¡
    .col-sm-6
      h3.text-right #{votes} comparisons made
  table.table.table-hover
    thead
      tr
        th #
        th Code
        th Name
        th Deviation
        th Rating
    tbody
      each _class, index in classes
        tr
          th= index
          td= _class.code
          td= _class.name
          td= "+/- " + Math.round(_class.rd)
          td= Math.round(_class.rating)
    
  
